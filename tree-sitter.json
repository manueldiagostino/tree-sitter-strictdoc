{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "strictdoc",
      "camelcase": "Strictdoc",
      "title": "Strictdoc",
      "scope": "source.strictdoc",
      "file-types": ["sdoc"],
      "first-line-regex": "^\\[DOCUMENT\\]",
      "injection-regex": "^strictdoc$",
      "class-name": "TreeSitterStrictdoc",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for the strictdoc language",
    "authors": [
      {
        "name": "Manuel Di Agostino",
        "email": "diagostinomanuel@gmail.com",
        "url": "https://www.manueldiagostino.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-strictdoc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}

